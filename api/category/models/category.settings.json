{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "name": "Category"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "products": {
      "via": "category_id",
      "collection": "product"
    },
    "coupon_coffees": {
      "via": "category_applicables",
      "collection": "coupon-coffee"
    }
  }
}
